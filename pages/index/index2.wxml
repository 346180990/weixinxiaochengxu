<view class="search_input">
      <input auto-focus placeholder="搜索车辆"/>
</view>
<view class="">
  <view class="table">
    <view class="tr bg-w">
    <view class="th1">车辆编号</view>
    <view class="th2">上传者</view>
    <view class="th3">日期</view>
    </view>
    <block wx:for="{{listData}}" wx:key="{[code]}">
    <view class="tr bg-g" wx:if="{{index % 2 == 0}}">
    <view class="td1">{{item.code}}</view>
    <view class="td2">{{item.name}}</view>
    <view class="td3">{{item.date}}</view>
    </view>
    <view class="tr" wx:else>
    <view class="td1">{{item.code}}</view>
    <view class="td2">{{item.name}}</view>
    <view class="td3">{{item.date}}</view>
    </view>
    </block>
   </view>
</view>